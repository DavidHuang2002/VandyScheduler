{
   "manifest_version": 2,
   "name": "Vandy Scheduler", 
   "version": "1.0.0",
   "description": "Takes classes and organizes them into a schedule and puts classes into the class cart.",
   "icons": {
      "16": "schedule-icon16.png",
      "48": "schedule-icon48.png",
      "128": "schedule-icon128.png"
   },
   "content_scripts": [
      {
         "matches": ["https://webapp.mis.vanderbilt.edu/more/SearchClasses*"],
			"js": ["jquery-3.2.1.min.js", "content.js", "class.js", "schedule.js"],
         "css": ["myButton.css", "modal.css"],
			"all_frames": true
      }
   ],
   "permissions": [
      "tabs", "https://webapp.mis.vanderbilt.edu/more/SearchClasses*"
   ],
   "web_accessible_resources": ["comment-pic2.png", "comment-pic3.png"]

   
}